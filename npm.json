{
  "vue": {
    "destination": "nicegui/static",
    "keep": ["package/dist/vue\\.global(\\.prod)?\\.js"],
    "rename": {
      "package/dist": ".."
    }
  },
  "quasar": {
    "destination": "nicegui/static",
