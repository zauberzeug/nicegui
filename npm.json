      "package/dist/quasar(\\.prod)?\\.css",
      "package/dist/lang/.*\\.umd\\.prod\\.js"
    ],
    "rename": {
      "package/dist": ".."
    }
  },
  "tailwindcss": {
    "destination": "nicegui/static",
    "keep": [],
